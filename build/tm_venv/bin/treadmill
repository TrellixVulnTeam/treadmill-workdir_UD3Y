#!/opt/treadmill/tm_venv/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'Treadmill','console_scripts','treadmill'
__requires__ = 'Treadmill'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Treadmill', 'console_scripts', 'treadmill')()
    )
